pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                checkout scmGit(branches: [[name: '*/dev']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/ma1456/springboot.git']])
            }
        }
       stage('build') { 
       withMaven(jdk: 'java', maven: 'maven') { 
       println "build is running" 
       sh "mvn clean package" 
       } }
      } 
  }

